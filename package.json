{
  "name": "node-haste",
  "version": "2.12.0",
  "main": "js/index.js",
  "lotus": {
    "plugins": [
      "lotus-babel"
    ],
    "babel": {
      "ignore": [
        "**/__(tests|mocks)__/**/*"
      ]
    },
    "implicitDependencies": [
      "child_process",
      "crypto",
      "fs",
      "os",
      "path",
      "util"
    ]
  },
  "dependencies": {
    "LazyVar": "aleclarson/LazyVar#2.1.0",
    "Promise": "aleclarson/Promise#1.0.0",
    "PureObject": "aleclarson/PureObject#1.0.0",
    "Type": "aleclarson/Type#1.1.0",
    "assert": "aleclarson/assert#1.0.0",
    "assertType": "aleclarson/assertType#2.0.0",
    "assertTypes": "aleclarson/assertTypes#2.0.0",
    "debug": "^2.2.0",
    "emptyFunction": "aleclarson/emptyFunction#1.0.0",
    "fromArgs": "aleclarson/fromArgs#1.0.0",
    "graceful-fs": "^4.1.4",
    "has": "aleclarson/has#1.0.0",
    "in-array": "aleclarson/in-array#1.0.0",
    "io": "aleclarson/io#2.1.0",
    "json-stable-stringify": "^1.0.1",
    "mergeDefaults": "aleclarson/mergeDefaults#1.0.0",
    "node-paths": "aleclarson/node-paths#0.0.1",
    "sane": "^1.3.1",
    "sync": "aleclarson/sync#1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "aleclarson/babel-core#6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^9.0.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-types": "^6.5.2",
    "eslint": "^1.10.3",
    "fbjs-scripts": "^0.5.0",
    "jest-cli": "^0.9.2"
  },
  "jest": {
    "rootDir": "./src",
    "unmockedModulePathPatterns": [
      "denodeify",
      "fastpath"
    ]
  }
}
